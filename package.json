{
  "private": true,
  "scripts": {
    "build": "pnpm run --recursive --filter \"./packages/*\" --filter \"./references/packages/*\" build",
    "clean": "pnpm run --recursive --parallel clean",
    "dev": "pnpm --filter vite-react dev",
    "dev:core": "pnpm --filter vite-core dev",
    "format": "rome format . --write",
    "lint": "rome check .",
    "lint:fix": "pnpm lint --apply",
    "preinstall": "npx only-allow pnpm",
    "prepare": "npx simple-git-hooks",
    "test": "pnpm run --recursive --filter \"./packages/*\" --filter \"./references/packages/*\" test",
    "test:coverage": "pnpm run --recursive --filter \"./packages/*\" --filter \"./references/packages/*\" test:coverage",
    "typecheck": "pnpm run --recursive --parallel typecheck"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "2.25.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "17.0.21",
    "@viem/anvil": "0.0.5",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "bun": "^0.5.5",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.3",
    "msw": "^1.2.1",
    "rimraf": "^4.1.2",
    "rome": "^12.1.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "viem": "portable-types",
    "vite": "^4.0.4",
    "vitest": "^0.25.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm format && pnpm lint:fix"
  },
  "packageManager": "pnpm@8.3.1",
  "pnpm": {
    "patchedDependencies": {
      "@tanstack/query-core@5.0.0-alpha.26": "patches/@tanstack__query-core@5.0.0-alpha.26.patch"
    }
  }
}
