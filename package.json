{
  "private": true,
  "scripts": {
    "build": "pnpm run --r --filter wagmi... build",
    "clean": "pnpm run --r --parallel clean",
    "dev": "pnpm --filter vite-react dev",
    "dev:core": "pnpm --filter vite-core dev",
    "format": "rome format . --write",
    "lint": "rome check .",
    "lint:fix": "pnpm lint --apply",
    "preconstruct": "bun scripts/preconstruct.ts",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm preconstruct",
    "prepare": "npx simple-git-hooks",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:ui:cov": "vite preview --outDir ./coverage",
    "test:update": "vitest --update",
    "typecheck": "pnpm run --r --parallel typecheck"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "^2.26.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.9",
    "@viem/anvil": "0.0.5",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "@wagmi/chains": "workspace:*",
    "@wagmi/connectors": "workspace:*",
    "bun": "^0.5.9",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.4",
    "jsdom": "^20.0.3",
    "msw": "^1.2.1",
    "rimraf": "^4.4.1",
    "rome": "^12.1.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "viem": "0.0.0-portable-types-20230513210020",
    "vite": "^4.3.2",
    "vitest": "^0.31.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm format && pnpm lint:fix"
  },
  "packageManager": "pnpm@8.3.1",
  "pnpm": {
    "patchedDependencies": {
      "@tanstack/query-core@5.0.0-alpha.26": "patches/@tanstack__query-core@5.0.0-alpha.26.patch"
    }
  }
}
